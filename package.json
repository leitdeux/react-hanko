{
  "name": "react-hanko",
  "version": "0.1.0",
  "description": "A React component for rendering Japanese-style name seals (hanko)",
  "author": "Elliott Fiedler <elliott.fiedler@icloud.com> (https://github.com/leitdeux)",
  "main": "./lib/Hanko.js",
  "keywords": [
    "react",
    "react-component",
    "japanese",
    "hanko",
    "inkan",
    "names",
    "ui"
  ],
  "license": "MIT",
  "scripts": {
    "build": "BABEL_ENV=production babel src/ --out-dir lib --copy-files --ignore \"**/__tests__/*\"",
    "clean": "rimraf lib",
    "docs": "webpack-dev-server --hot --config docs/webpack.config.dev.js",
    "docs:build": "webpack -p --config docs/webpack.config.prod.js",
    "docs:deploy": "npm run docs:build && gh-pages -d docs",
    "prebuild": "npm run clean",
    "start": "npm run docs",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:unit": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0-alpha.3cc09dcf",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^20.0.4",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "testEnvironment": "node",
    "verbose": false
  }
}
